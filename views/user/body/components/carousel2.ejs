<div id="carouselLoginSlide" class="carousel slide h-100" data-bs-ride="carousel" data-bs-interval="3000">
    <div class="carousel-inner h-100">
        <div class="carousel-item active h-100">
            <img src="<%=carousalImages[0]%>" class="d-block w-100 h-100" style="object-fit: cover;" alt="Book Collection">
        </div>
        <% for(let i = 1; i < carousalImages.length; i++){%>
            <div class="carousel-item h-100">
                <img src="<%= carousalImages[i] %>" class="d-block w-100 h-100" style="object-fit: cover;" alt="Book Collection">
            </div>
        <%}%>
    </div>
</div>